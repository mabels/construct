# <%= @entry.iface.clazz %>
<% if @auto -%>
auto <%= get_interface_name %>
<% end -%>
iface <%= get_interface_name %> <%= @protocol %> <%= @mode %>
  up   /bin/bash /etc/network/<%= get_interface_name %>-up.iface
<% Lines.ordered_lines(@post_ups).each do |post_up| -%>
  post-up <%= post_up %>
<% end -%>
<% Lines.ordered_lines(@pre_downs).each do |pre_down| -%>
  pre-down <%= pre_down %>
<% end -%>
  down /bin/bash /etc/network/<%= get_interface_name %>-down.iface
