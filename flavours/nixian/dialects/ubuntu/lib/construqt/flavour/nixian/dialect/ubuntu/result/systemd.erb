[Unit]
Description=<%= self.get_description %>
<% @befores.each do |before| -%>
Before=<%= before %>
<% end -%>

[Service]
Type=<%= @type %>
<% if !@exec_start.nil? && !@exec_start.empty? -%>
ExecStart=<%= @exec_start %>
<% end -%>
<% if !@exec_stop.nil? && !@exec_stop.empty? -%>
ExecStop=<%= @exec_stop %>
<% end -%>

[Install]
<% @wanted_bys.each do |wanted_by| -%>
WantedBy=<%= wanted_by %>
<% end -%>
<% @alsos.each do |also| -%>
Also=<%= also %>
<% end -%>
