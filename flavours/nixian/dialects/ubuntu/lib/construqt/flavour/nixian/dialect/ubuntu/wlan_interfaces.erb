#wpa-driver wext
#wpa-ap-scan 1
#wpa-proto RSN
#wpa-pairwise CCMP
#wpa-group CCMP
wpa-ssid <%= wlan_delegate.ssid %>
wpa-key-mgmt WPA-PSK
wpa-psk <%= OpenSSL::PKCS5.pbkdf2_hmac_sha1(wlan_delegate.psk,
              wlan_delegate.ssid, 4096, 32)
              .bytes.to_a.map{|i| "%02x"%i}.join("") %>
